{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "public/index.html"
    },
    {
      "src": "/api/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/markdown/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/uploads/(.*)",
      "dest": "index.js"
    }
  ]
}
